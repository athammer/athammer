{
  "name": "athammer",
  "version": "0.0.0",
  "author": "Aaron T Hammer athammer@buffalo.edu",
  "description": "my site :)",
  "main": "server.js",
  "scripts": {
    "start": "npm run sass & npm run watch-sass & npm run watch-webpack & pm2 start server.js --watch",
    "globals": "npm install pm2 -g",
    "logs": "pm2 logs",
    "build": "./node_modules/webpack/bin/webpack.js -p",
    "webpack": "./node_modules/webpack/bin/webpack.js",
    "watch-webpack": "./node_modules/webpack/bin/webpack.js -d --watch --output-pathinfo",
    "watch-sass": "./node_modules/node-sass/bin/node-sass -w src/sass/main.scss -o public/stylesheets/ --source-map public/stylesheets/",
    "sass": "./node_modules/node-sass/bin/node-sass src/sass/main.scss -o public/stylesheets/ --source-map public/stylesheets/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-handlebars": "^3.0.0",
    "helmet": "^3.13.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "serve-favicon": "^2.5.0",
    "validator": "^10.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.2.0"
  }
}
